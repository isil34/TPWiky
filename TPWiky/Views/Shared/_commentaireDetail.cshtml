@model Commentaire

<table class="table table-striped table-sm table-bordered text-center">
    <thead>
        <tr>
            <th>Auteur</th>
            <th>Contenu</th>
            <th>Date de création</th>
            <th>Date de Modification</th>
        </tr>
    </thead>
    <tbody id="contentCommentaire">

        <tr>
            <td>
                @Model.Id
            </td>
            <td>
                @Model.Auteur
            </td>
            <td>
                @Model.Contenu
            </td>
            <td>
                @Model.DateCreation
            </td>
            <td>
                @Model.DateModification
            </td>
        </tr>

    </tbody>
</table>